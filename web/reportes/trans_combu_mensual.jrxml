	<queryString>
		<![CDATA[select m.placa as placa,
         extract(month from a.fecha) as fecha,
         to_char(a.fecha, 'TMMonth') as periodo,
         SUM(a.monto) as gasto
    from control_combustible as a
    join unidad_transporte as m
      on a.placa = m.placa
   where a.fecha >= $P{fechaInicio}
     and a.fecha <= $P{fechaFin}
     and m.placa in ($P!{placas})
group by m.placa, extract(month from a.fecha), periodo
order by fecha;]]>
	</queryString>
