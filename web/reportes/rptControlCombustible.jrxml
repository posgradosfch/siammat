	<queryString>
		<![CDATA[SELECT
CASE (SELECT extract(MONTH FROM fecha))

   WHEN 1 THEN 'Enero'

   WHEN 2 THEN 'Febrero'

   WHEN 3 THEN 'Marzo'

   WHEN 4 THEN 'Abril'

   WHEN 5 THEN 'Mayo'

   WHEN 6 THEN 'Junio'

   WHEN 7 THEN 'Julio'

